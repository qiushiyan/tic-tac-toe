{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "check:type": "tsc --noEmit",
    "check:format": "npx prettier --check '**/*.{ts,tsx,json}'",
    "format": "npx prettier --write '**/*.{ts,tsx,json}'",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "expo": "~43.0.2",
    "expo-status-bar": "~1.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "husky": "^7.0.4",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn check:format",
      "yarn check:type"
    ]
  }
}
